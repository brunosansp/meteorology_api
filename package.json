{
  "name": "meteorology-api",
  "version": "1.0.0",
  "description": "NodeJS API with Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev --transpile-only src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test:functional": "jest --projects ./test --forceExit --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunosansp/meteorology_api.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Bruno Santos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brunosansp/meteorology_api/issues"
  },
  "homepage": "https://github.com/brunosansp/meteorology_api#readme",
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.7.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "jest": "^28.1.3",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  }
}